{
    "template": "<div class=\"section\" ng-init=\"adminControls=false\">\n  <div class=\"sectionIconContainer\"><div class=\"sectionIcon\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#line-visuals\"></use></svg></div></div>\n  <div class=\"widget\">\n    <div class=\"title\" ng-click=\"adminControls=!adminControls\">\n      <div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#down-arrow-2\"></use></svg></div>\n      Admin Controls\n    </div>\n\n    <div class=\"valueGroup\" ng-if=\"adminControls==true\" >\n      <div class=\"sceneGroup\" style=\"size: 18;\">\n        <div class=\"option link\"><a href=\"http://debra:8080\" target=\"_blank\">Open Home page</a></div>\n        <div class=\"option link\"><a href=\"http://debra:8080/doc/index.html\" target=\"_blank\">REST API</a></div>\n        <div class=\"option link\"><a href=\" http://debra:8080/paperui/index.html\" target=\"_blank\">Paper UI</a></div>\n        <div class=\"option link\"><a href=\"http://debra:9001\" target=\"_blank\">Link to log</a></div>\n      </div>\n    </div>\n  </div>\n  <div ng-if=\"adminControls==true\">\n    <div class=\"widget\">\n      <div class=\"title\">Experimental&#13;</div>\n      <div class=\"sceneGroup\">\n        <div class=\"option\">\n          <div class=\"btnwidget\" oc-lazy-load=\"['/static/js/habpanel-refreshbutton.js']\">\n            <div class=\"flex\">\n              <button refresh-button class=\"btn2\" style=\"outline: none; background-color: transparent; border: 0px; flex: 1 1 auto\">\n                Reload Panel\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n<div class=\"section\">\n  <div class=\"thing\"></div>\n\n  <div class=\"sectionIconContainer\"><div class=\"sectionIcon\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#line-visuals\"></use></svg></div></div>\n  <div class=\"title\">Music</div>\n  <div class=\"controls\">\n\n    <div class=\"widget\" >\n      <div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#line-visuals\"></use></svg></div>\n      <div class=\"name\">Track</div>\n      <div class=\"appLogo\"><svg viewBox=\"0 0 440 100\"><use xlink:href=\"/static/matrix-theme/AppIcons.svg#spotify\"></use></svg></div>        \n      <div class=\"valueGroup\">\n        <div class=\"value\">{{itemValue('spotify_current_track')}}</div>\n        <div class=\"value\">{{itemValue('spotify_current_artist')}}</div>\n      </div>\n      <div class=\"controlGroup\" style=\"padding: 1px 1px 1px 1px;\">\n        <div class=\"music\">\n          <div class=\"cover\" ng-style=\"{'background-image': 'url(' + itemValue('spotify_current_cover') + ')'}\"></div>\n          <div class=\"playback\" oc-lazy-load=\"['/static/js/myslider.js']\" >\n\n            <div ng-app=\"rzSliderDemo\">\n              <div ng-controller=\"MainCtrl\" class=\"wrapper\">\n                <div class=\"name\" ng-init=\"stoggle=true\" ></div>\n                <div ng-model=\"slider_callbacks.options.ceil=(itemValue('spotify_current_duration') - itemValue('spotify_current_duration')%1000 )/1000\"></div>\n                <div ng-if=\"stoggle!=false\" ng-model=\"slider_callbacks.value=itemValue('spotify_current_progress_estimated')\"></div>\n                <div ng-if=\"stoggle==false\" ng-init=\"slider_callbacks.value=itemValue('spotify_current_progress_estimated')\"></div>\n                <rzslider  rz-slider-model=\"slider_callbacks.value\"\n                          rz-slider-options=\"slider_callbacks.options\"  ng-click=\"sendCmd('spotify_current_progress_estimated',slider_callbacks.value)\"></rzslider>\n              </div>\n            </div>\n          </div>\n        </div>\n\n\n        <div class=\"control\" ng-click=\"sendCmd('spotify_action', 'next')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#right-arrow-2\"></use></svg></div>\n        <div class=\"control\" ng-click=\"sendCmd('spotify_action', 'play')\" ng-if=\"itemValue('spotify_current_playing')=='OFF'\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#right-play\"></use></svg></div>\n        <div class=\"control\" ng-click=\"sendCmd('spotify_action', 'pause')\" ng-if=\"itemValue('spotify_current_playing')=='ON'\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#pause\"></use></svg></div>\n        <div class=\"control\" ng-click=\"sendCmd('spotify_action', 'previous')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#left-arrow-2\"></use></svg></div>\n      </div>\n    </div> \n\n    <div class=\"widget\" ng-init=\"deviceVolume={ value: itemValue('spotify_current_volume') }\">\n      <div class=\"icon off\"  ><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume\"></use></svg></div>\n      <div ng-if=\"itemValue('spotify_current_device') != 'CAIN'\" ng-model=\"deviceVolume.value=itemValue('spotify_current_volume')\" ></div >\n      <div ng-if=\"itemValue('spotify_current_device') == 'CAIN'\" ng-model=\"deviceVolume.value=((1000 * itemValue('LG_TV0_Volume'))/ 10)\" ></div >\n      <div class=\"name\">Volume<div class=\"value\">{{deviceVolume.value }} </div></div>\n      <div class=\"controlGroup\">\n        <div class=\"control\" ng-click=\"sendCmd('spotify_set_volume', 'up')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#top-arrow-2\"></use></svg></div>\n        <div class=\"control\" ng-click=\"sendCmd('spotify_set_volume', 'volume_down')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#down-arrow-2\"></use></svg></div>\n        <div class=\"control on\" ng-if=\"itemValue('spotify_current_volume')=='0'\" ng-click=\"sendCmd('spotify_set_volume', 'mute')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume-close\"></use></svg></div>\n        <div class=\"control\" ng-if=\"itemValue('spotify_current_volume')!='0'\" ng-click=\"sendCmd('spotify_set_volume', 'mute')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume\"></use></svg></div>\n\n\n        <div class=\"playback\" oc-lazy-load=\"['/static/js/myslider.js']\" >\n          <div ng-app=\"rzSliderDemo\">\n            <div ng-controller=\"MainCtrl\" class=\"wrapper\">\n              <div class=\"name\" ></div>\n              <div ng-init=\"slider_volume.toggle=true\"></div>\n              <div ng-if=\"slider_volume.toggle==false\" ng-init=\"slider_volume.value=deviceVolume.value\"></div>\n              <div ng-if=\"slider_volume.toggle!=false\" ng-model=\"slider_volume.value=deviceVolume.value\"></div>\n              <rzslider  rz-slider-model=\"slider_volume.value\"\n                        rz-slider-options=\"slider_volume.options\"  ng-click=\"sendCmd('spotify_set_volume', slider_volume.value)\"></rzslider>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"widget\" ng-click=\"sendCmd('spotify_forceupadte', 'ON')\">\n      <div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#double-arrow\"></use></svg></div>\n      <div class=\"nameGroup\"><div class=\"name\">Update</div></div>\n      <div class=\"valueGroup\"><div class=\"value\">{{itemValue('spotify_lastConnectionDateTime') | date:'MMM d, HH:mm:ss'}}</div></div>\t\n    </div>  \n\n    <div class=\"widget\" ng-init=\"devices=$eval(itemValue('spotify_devices')).devices\">\n      <div class=\"icon off\" ng-click=\"sendCmd('spotify_device_update', 'ON'); devices=$eval(itemValue('spotify_devices')).devices\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#double-arrow\"></use></svg></div>\n      <div class=\"nameGroup\"><div class=\"name\">Devices</div></div>\n      <div class=\"btn-group\" dropdown-append-to-body=\"true\" uib-dropdown >            \n        <button id=\"single-button\" type=\"button\" class=\"btn btn-primary\" uib-dropdown-toggle  ng-click=\"devices=$eval(itemValue('spotify_devices')).devices\" ng-mouseover=\"devices=$eval(itemValue('spotify_devices')).devices\">\n          {{itemValue('spotify_current_device')}} <span class=\"caret\"></span>\n        </button>\n        <ul class=\"mydropdown-menu\" uib-dropdown-menu role=\"menu\" style=\"overflow: auto; max-height:300px; overflow-x:hidden\" aria-labelledby=\"single-button\" >   \n          <div ng-repeat=\"dev in devices\" ng-if=\"dev.state!='NULL'\" >\n            <li role=\"menuitem\"><a ng-click='sendCmd(\"spotify_action\", \"play \\\"\" + dev.id + \"\\\"\")'>{{dev.name}}</a></li>\n          </div>\n        </ul>\n      </div>\n    </div>\n\n    <div class=\"widget\" >\n      <div class=\"icon off\" ng-click=\"sendCmd('spotify_action', 'playlists'); playlists=$eval(itemValue('spotify_playlists')).items; currenturi=itemValue('spotify_current_context_uri')\" ><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#double-arrow\"></use></svg></div>\n      <div class=\"nameGroup\" ng-init=\"playlistDetails = { name: 'Select', uri: '0' } ; playlists=$eval(itemValue('spotify_playlists')).items ; currenturi=itemValue('spotify_current_context_uri')\" ><div class=\"name\">Playlists </div>\n        <div ng-repeat=\"song in playlists | filter: {uri: currenturi } \"><div ng-init=\"playlistDetails.name=song.name; playlistDetails.uri=song.uri\"></div></div>\n        <div ng-if=\"currenturi != playlistDetails.uri\"><div ng-init=\"playlistDetails.name='';playlistDetails.uri=''\"></div> </div>\n        <select ng-options=\"item as item.name for item in playlists track by item.uri\" ng-model=\"playlistDetails\" ng-mouseover=\"currenturi=itemValue('spotify_current_context_uri')\" ng-click=\"currenturi=itemValue('spotify_current_context_uri')\" ng-change='sendCmd(\"spotify_action\", \"play \\\"\" + playlistDetails.uri + \"\\\"\")'>  <option value=\"\">Select playlist</option>{{playlistDetails.name}}</select>\n      </div>\n    </div>\n\n    <div class=\"widget\" >\n      <div class=\"icon off\" ng-if=\"itemValue('spotify_lock') < 51\" ng-click=\"sendCmd('spotify_lock', 100)\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#double-arrow\"></use></svg></div>\n      <div class=\"icon on\" ng-if=\"!(itemValue('spotify_lock')  < 51)\" ng-click=\"sendCmd('spotify_lock', 50)\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#double-arrow\"></use></svg></div>\n      <div class=\"nameGroup\"><div class=\"name\">Lock</div></div>\n      <div class=\"valueGroup\"><div class=\"value\">{{itemValue('spotify_lock')}}</div></div>\t\n    </div>  \n\n\n  </div>   \n</div>\n\n\n\n\n  \n \n\n<div class=\"section\">\n\n\t<div class=\"sectionIconContainer\"><div class=\"sectionIcon\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#flat-tv\"></use></svg></div></div>\n\t<div class=\"title\">TV</div>\n\t<div class=\"controls\">\n    \n     \n      \n    <div class=\"widget\" ng-if=\"itemValue('HarmonyActivity')=='PowerOff'\" ng-click=\"sendCmd('HarmonyActivity', 'PC')\">\n      <div class=\"icon off\" ><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#off\"></use></svg></div>\n      <div class=\"name\">Power On</div>\n    </div>\n\n    <div class=\"widget\" ng-if=\"itemValue('HarmonyActivity')!='PowerOff'\" ng-click=\"sendCmd('HarmonyActivity', 'PowerOff')\">\n      <div class=\"icon on\" ><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#on\"></use></svg></div>\n      <div class=\"name\">State: {{itemValue('HarmonyActivity')}}</div>\n    </div>\n\n    <div class=\"widget\" ng-if=\"itemValue('LG_TV0_Power')=='NULL'\" ng-click=\"sendCmd('HarmonyActivity', 'PC')\">\n      <div class=\"icon off\" ><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#none\"></use></svg></div>\n      <div class=\"name\">Power</div>\n    </div>                \n\n    <div class=\"widget\" ng-class=\"{true: 'disabled'}[itemValue('HarmonyActivity')=='PowerOff']\">\n\t\t\t<div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#flat-tv\"></use></svg></div>\n\t\t\t<div class=\"name\">Input</div>\n\t\t\t<div class=\"appLogo\" ng-if=\"itemValue('LG_TV0_Application')=='netflix'\"><svg viewBox=\"0 0 440 100\"><use xlink:href=\"/static/matrix-theme/AppIcons.svg#netflix\"></use></svg></div>        \n\t\t\t<div class=\"appLogo\" ng-if=\"itemValue('LG_TV0_Application')=='lovefilm'\"><svg viewBox=\"0 0 440 100\"><use xlink:href=\"/static/matrix-theme/AppIcons.svg#amazon\"></use></svg></div>        \n\t\t\t<div class=\"appLogo\" ng-if=\"itemValue('LG_TV0_Application')=='Youtube'\"><svg viewBox=\"0 0 440 100\"><use xlink:href=\"/static/matrix-theme/AppIcons.svg#youtube\"></use></svg></div>        \n\t\t\t<div class=\"appLogo\" ng-if=\"itemValue('LG_TV0_Application')=='com.webos.app.hdmi1'\">PC<svg viewBox=\"0 0 180 180\"><use xlink:href=\"/static/matrix-theme/squidink.svg#processor\"></use></svg></div>        \n\t\t\t<div class=\"appLogo\" ng-if=\"itemValue('LG_TV0_Application')!='netflix' && itemValue('LG_TV0_Application')!='lovefilm' && itemValue('LG_TV0_Application')!='Youtube' && itemValue('LG_TV0_Application')!=com.webos.app.hdmi1'\">{{itemValue('LG_TV0_Application')}} {{itemValue('HarmonyActivity}}</div>        \n\t\t\t<div class=\"controlGroup\">\n\t\t\t\t<div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HarmonyActivity')=='Energy']\" ng-click=\"sendCmd('HarmonyActivity', 'Energy')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#logoYoutube\"></use></svg></div>\n\t\t\t\t<div class=\"control\" ng-class=\"{true: 'on'}[itemValue('LG_TV0_Application')=='lovefilm']\" ng-click=\"sendCmd('LG_TV0_Application', 'lovefilm')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#logoAmazon\"></use></svg></div>\n\t\t\t\t<div class=\"control\" ng-class=\"{true: 'on'}[itemValue('LG_TV0_Application')=='com.webos.app.hdmi1']\" ng-click=\"sendCmd('LG_TV0_Application', 'com.webos.app.hdmi1')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#processor\"></use></svg></div>\n\t\t\t\t<div class=\"control\" ng-class=\"{true: 'on'}[itemValue('LG_TV0_Application')=='netflix']\" ng-click=\"sendCmd('LG_TV0_Application', 'netflix')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#logoNetflix\"></use></svg></div>\n\t\t\t\t<div class=\"control\" ng-class=\"{true: 'on'}[itemValue('HarmonyActivity')=='Watch PC']\" ng-click=\"sendCmd('HarmonyActivity', 'Watch PC')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#flat-tv\"></use></svg></div>\n\t\t\t</div>\n\t\t</div>\n\n    \n    <div class=\"widget\">\n      <div class=\"icon off\" ng-if=\"itemValue('HarmonySamsung')=='InputHdmi1'\" ng-click=\"sendCmd('HarmonySamsung', 'InputHdmi2')\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#double-arrow\"></use></svg></div>\n      <div class=\"icon on\" ng-if=\"itemValue('HarmonySamsung')!='InputHdmi1'\" ng-click=\"sendCmd('HarmonySamsung', 'InputHdmi1')\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#double-arrow\"></use></svg></div>\n\t\t\t<div class=\"name\">SamsungTV Toggle HMDI Input - {{itemValue('HarmonySamsung')}}</div>\n\t\t</div>\n    \n    \n    <div class=\"widget\" ng-class=\"{true: 'disabled'}[itemValue('HarmonyActivity')=='PowerOff']\">\n\t\t\t<div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume\"></use></svg></div>\n\t\t\t<div class=\"name\">Volume<div class=\"value\">{{itemValue('LG_TV0_Volume') * 100 | number:0}}</div></div>\n\t\t\t<div class=\"controlGroup\">\n\t\t\t\t<div class=\"control\" ng-click=\"sendCmd('LG_TV0_VolumeUp', 'ON')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#top-arrow-2\"></use></svg></div>\n\t\t\t\t<div class=\"control\" ng-click=\"sendCmd('LG_TV0_VolumeDown', 'ON')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#down-arrow-2\"></use></svg></div>\n\t\t\t\t<div class=\"control on\" ng-if=\"itemValue('LG_TV0_Mute')=='ON'\" ng-click=\"sendCmd('LG_TV0_Mute', 'OFF')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume-close\"></use></svg></div>\n\t\t\t\t<div class=\"control\" ng-if=\"itemValue('LG_TV0_Mute')!='ON'\" ng-click=\"sendCmd('LG_TV0_Mute', 'ON')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume\"></use></svg></div>\n\t\t\t</div>\n\t\t</div>\n    \n\n    <div class=\"widget\" ng-class=\"{true: 'disabled'}[itemValue('HarmonyActivity')=='PowerOff']\">\n\t\t\t<div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume\"></use></svg></div>\n\t\t\t<div class=\"name\" ng-init=\"harmonyVolume=false\" ng-click=\"harmonyVolume=!harmonyVolume\">Enable Harmony Volume<div class=\"sicon on\" ng-if=\"harmonyVolume==false\"><svg  viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#down-arrow-2\"></use></svg></div><div class=\"sicon on\" ng-if=\"harmonyVolume==true\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#top-arrow-2\"></use></svg></div></div>\n\t\t\t<div class=\"controlGroup\" ng-if=\"harmonyVolume==true\">\n\t\t\t\t<div class=\"control\" ng-click=\"sendCmd('harmonyhub_device_Master_36738743_buttonPress', 'VolumeUp')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#top-arrow-2\"></use></svg></div>\n\t\t\t\t<div class=\"control\" ng-click=\"sendCmd('harmonyhub_device_Master_36738743_buttonPress', 'VolumeDown')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#down-arrow-2\"></use></svg></div>\n\t\t\t\t<div class=\"control on\" ng-if=\"itemValue('LG_TV0_Mute')=='ON'\" ng-click=\"sendCmd('harmonyhub_device_Master_36738743_buttonPress', 'mute')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume\"></use></svg></div>\n\t\t\t\t<div class=\"control\" ng-if=\"itemValue('LG_TV0_Mute')!='ON'\" ng-click=\"sendCmd('harmonyhub_device_Master_36738743_buttonPress', 'mute')\"><svg viewBox=\"0 0 80 80\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume-close\"></use></svg></div>\n\t\t\t</div>\n\t\t</div>\n    \n    \n    \n    <div class=\"widget\" ng-class=\"{true: 'disabled'}[itemValue('HarmonyActivity')=='PowerOff']\">\n      <div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#volume\"></use></svg></div>\n      <div class=\"name\" ng-init=\"soundOptions=false\" ng-click=\"soundOptions=!soundOptions\">Sound Options\n        <div class=\"sicon on\" ng-if=\"soundOptions==false\"><svg  viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#down-arrow-2\"></use></svg></div><div class=\"sicon on\" ng-if=\"soundOptions==true\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#top-arrow-2\"></use></svg></div>\n      </div>\n      <div class=\"sceneGroup\" ng-if=\"soundOptions==true\">\n        <div class=\"scene\" ng-click=\"sendCmd('HarmonyYamaha', 'DspStraight')\">Straight</div>\n        <div class=\"scene\" ng-click=\"sendCmd('HarmonyYamaha', 'InputHdmi1')\">HDMI 1</div>\n        <div class=\"scene\" ng-click=\"sendCmd('HarmonyYamaha', 'InputAudio')\">App sounds</div>\n        <div class=\"scene\" ng-click=\"sendCmd('HarmonyYamaha', 'DspMusic')\">Music</div>\n        <div class=\"scene\" ng-click=\"sendCmd('HarmonyYamaha', 'DspMovie')\">Movie</div>\n      </div>\n    </div>\n   \n    <div class=\"remotes\" ng-init=\"remotep=false; remotel=false\">\n      <div class=\"widget\">\n        <div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#flat-tv\"></use></svg></div>\n        <div class=\"name\">Channel</div>\n        <div class=\"name\"  ng-click=\"remotep=!remotep\">Enable Remotes<div class=\"sicon on\" ng-if=\"remotep==false\"><svg  viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#down-arrow-2\"></use></svg></div><div class=\"sicon on\" ng-if=\"remotep!=false\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#top-arrow-2\"></use></svg></div></div>\n      </div>\n      <div class=\"widget-wide\" ng-show=\"remotep !=false\"> \n        <div ng-repeat=\"item in itemsInGroup('gharmony')\">\n          <div class=\"title\">&#13; Remote: {{item.label}}&#13;</div>       \n          <div class=\"col-md-12\">\n            <div ng-if=\"remotel\" ng-repeat=\"i in item.stateDescription.options\">   \n              <div class=\"option button\" ng-click='sendCmd(item.name, i.value)'>{{i.label}}</div>\n            </div>\n          </div><div class=\"title\" ng-show=\"remotep == true\" ng-click=\"remotel=!remotel\"><div class=\"value\">Toggle Remote <div class=\"toggleIcon\" style=\"display: inline-block;\"><div class=\"sicon on\"><svg  viewBox=\"0 0 48 36\"><use  ng-if=\"remotel !=true\" xlink:href=\"/static/matrix-theme/squidink.svg#down-arrow-2\"></use><use ng-if=\"remotel==true\" xlink:href=\"/static/matrix-theme/squidink.svg#top-arrow-2\"></use></svg></div></div> </div>       \n        </div></div>\n        <div class=\"title\" ng-show=\"remotep == true\" ng-click=\"remotel=false; remotep=false\"><div class=\"value\">Close ALL Remotes <div class=\"toggleIcon\"><div class=\"sicon on\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#top-arrow-2\"></use></svg></div></div></div>\n      </div>  </div>\n    </div>   \n